Cell[CellGroupData[{
Cell["Example 2: Harmonic Oscillator", "Section",
 CellChangeTimes->{{3.9*^9, 3.9*^9}},ExpressionUUID->"l2m3n4o5-p6q7-8901-lmno-234567890123"],

Cell[TextData[{
 "Solve the second-order ODE: d\[Squared]y/dt\[Squared] + \[Omega]\[Squared]y = 0\n",
 "Convert to system: dy\[ScriptCapitalD]/dt = y\[ScriptCapitalE], dy\[ScriptCapitalE]/dt = -\[Omega]\[Squared]y\[ScriptCapitalD]\n",
 "Analytical solution: y(t) = A cos(\[Omega]t) + B sin(\[Omega]t)"
}], "Text",
 CellChangeTimes->{{3.9*^9, 3.9*^9}},ExpressionUUID->"m3n4o5p6-q7r8-9012-mnop-345678901234"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"\[Omega]", "=", "2.0"}], ";", " ", 
  RowBox[{"(*", " ", "Angular frequency", " ", "*)"}]}], "\n", 
 RowBox[{
  RowBox[{"A", "=", "1.0"}], ";", " ", 
  RowBox[{"(*", " ", "Amplitude for cosine", " ", "*)"}]}], "\n", 
 RowBox[{
  RowBox[{"B", "=", "0.5"}], ";", " ", 
  RowBox[{"(*", " ", "Amplitude for sine", " ", "*)"}]}], "\n", 
 RowBox[{
  RowBox[{"y0Harmonic", "=", 
   RowBox[{"{", 
    RowBox[{"A", ",", 
     RowBox[{"B", "*", "\[Omega]"}]}], "}"}]}], ";", " ", 
  RowBox[{"(*", " ", 
   RowBox[{"{", 
    RowBox[{"position", ",", " ", "velocity"}], "}"}], " ", "*)"}]}]}], "Input",
 CellChangeTimes->{{3.9*^9, 3.9*^9}},
 CellLabel->"In[20]:=",ExpressionUUID->"n4o5p6q7-r8s9-0123-nopq-456789012345"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"harmonicODE", "[", 
    RowBox[{"t_", ",", "y_"}], "]"}], ":=", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"y", "[", 
      RowBox[{"[", "2", "]"}], "]"}], ",", 
     RowBox[{
      RowBox[{"-", "\[Omega]"}], "^", 
      RowBox[{"2", "*", 
       RowBox[{"y", "[", 
        RowBox[{"[", "1", "]"}], "]"}]}]}]}], "}"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"analyticalPosition", "[", "t_", "]"}], ":=", 
   RowBox[{
    RowBox[{"A", "*", 
     RowBox[{"Cos", "[", 
      RowBox[{"\[Omega]", "*", "t"}], "]"}]}], "+", 
    RowBox[{"B", "*", 
     RowBox[{"Sin", "[", 
      RowBox[{"\[Omega]", "*", "t"}], "]"}]}]}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"analyticalVelocity", "[", "t_", "]"}], ":=", 
   RowBox[{
    RowBox[{
     RowBox[{"-", "A"}], "*", "\[Omega]", "*", 
     RowBox[{"Sin", "[", 
      RowBox[{"\[Omega]", "*", "t"}], "]"}]}], "+", 
    RowBox[{"B", "*", "\[Omega]", "*", 
     RowBox[{"Cos", "[", 
      RowBox[{"\[Omega]", "*", "t"}], "]"}]}]}]}], ";"}]}], "Input",
 CellChangeTimes->{{3.9*^9, 3.9*^9}},
 CellLabel->"In[24]:=",ExpressionUUID->"o5p6q7r8-s9t0-1234-opqr-567890123456"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"T", "=", 
   RowBox[{
    RowBox[{"2", "*", "Pi"}], "/", "\[Omega]"}]}], ";", " ", 
  RowBox[{"(*", " ", "Period", " ", "*)"}]}], "\n", 
 RowBox[{
  RowBox[{"tSpanHarmonic", "=", 
   RowBox[{"{", 
    RowBox[{"0", ",", "T"}], "}"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"dtHarmonic", "=", 
   RowBox[{"T", "/", "100"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"resultHarmonic", "=", 
   RowBox[{"RungeKutta4Solve", "[", 
    RowBox[{"harmonicODE", ",", "y0Harmonic", ",", 
     RowBox[{"{", 
      RowBox[{"t", ",", 
       RowBox[{"tSpanHarmonic", "[", 
        RowBox[{"[", "1", "]"}], "]"}], ",", 
       RowBox[{"tSpanHarmonic", "[", 
        RowBox[{"[", "2", "]"}], "]"}]}], "}"}], ",", "dtHarmonic"}], "]"}]}],
   ";"}]}], "Input",
 CellChangeTimes->{{3.9*^9, 3.9*^9}},
 CellLabel->"In[27]:=",ExpressionUUID->"p6q7r8s9-t0u1-2345-pqrs-678901234567"],

