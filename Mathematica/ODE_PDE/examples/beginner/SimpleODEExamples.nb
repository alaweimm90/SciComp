 RowBox[{
  RowBox[{
   RowBox[{"odeDecay", "[", 
    RowBox[{"t_", ",", "y_"}], "]"}], ":=", 
   RowBox[{"\[Lambda]Decay", "*", "y"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"analyticalGrowth", "[", "t_", "]"}], ":=", 
   RowBox[{"y0", "*", 
    RowBox[{"Exp", "[", 
     RowBox[{"\[Lambda]Growth", "*", "t"}], "]"}]}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"analyticalDecay", "[", "t_", "]"}], ":=", 
   RowBox[{"y0", "*", 
    RowBox[{"Exp", "[", 
     RowBox[{"\[Lambda]Decay", "*", "t"}], "]"}]}]}], ";"}]}], "Input",
