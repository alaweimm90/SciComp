Cell[CellGroupData[{
Cell["Example 1: Exponential Growth and Decay", "Section",
 CellChangeTimes->{{3.9*^9, 3.9*^9}},ExpressionUUID->"f6g7h8i9-j0k1-2345-fghi-678901234567"],

Cell[TextData[{
 "Solve the ODE: dy/dt = \[Lambda]y\n",
 "For \[Lambda] > 0: exponential growth\n",
 "For \[Lambda] < 0: exponential decay\n",
 "Analytical solution: y(t) = y\[ScriptCapitalD] exp(\[Lambda]t)"
}], "Text",
 CellChangeTimes->{{3.9*^9, 3.9*^9}},ExpressionUUID->"g7h8i9j0-k1l2-3456-ghij-789012345678"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"\[Lambda]Growth", "=", "0.5"}], ";", " ", 
  RowBox[{"(*", " ", "Growth rate", " ", "*)"}]}], "\n", 
 RowBox[{
  RowBox[{"\[Lambda]Decay", "=", 
   RowBox[{"-", "1.0"}]}], ";", " ", 
  RowBox[{"(*", " ", "Decay rate", " ", "*)"}]}], "\n", 
 RowBox[{
  RowBox[{"y0", "=", "1.0"}], ";", " ", 
  RowBox[{"(*", " ", "Initial condition", " ", "*)"}]}], "\n", 
 RowBox[{
  RowBox[{"tSpan", "=", 
   RowBox[{"{", 
    RowBox[{"0", ",", "3"}], "}"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"dt", "=", "0.1"}], ";"}]}], "Input",
 CellChangeTimes->{{3.9*^9, 3.9*^9}},
 CellLabel->"In[6]:=",ExpressionUUID->"h8i9j0k1-l2m3-4567-hijk-890123456789"],

